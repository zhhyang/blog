<div class="row">
    <ul class="pagination">
        {{#if isFirstPage}}

        {{else}}
        <li><a title="上一页"  href="?p=<%= page-1 %>">&laquo;</a></li>
        {{/if}}

        {{#if isLastPage}}
        {{else}}
        <li><a title="下一页" href="?p=<%= page+1 %>">&raquo;</a></li>
        {{/if}}
    </ul>
</div>